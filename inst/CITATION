year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{netReducer}: Analysing the impact of sample size and species composition in network analysis",
         author = c(person("David", "Hemprich-Bennett")),
         year = year,
         note = note,
         url = "https://github.com/hemprichbennett/netReducer",
         doi = "10.5281/zenodo.1290732")
